# 4.
print('4.')
a = input('Enter Integer: ')
n = int(a)
nn = int(a + a)
nnn = int(a + a + a)
res = n + nn + nnn
print(res)

# 5.
import math

print('5.')
r = 6
V = (4 / 3) * math.pi * (r ** 3)
print(V)

# 6.
print('6.')
n = eval(input('Enter Number: '))
if n > 17:
    d = n - 17
    finres = d * 2

print(finres)

#7.
print('7.')
a = eval(input('Number A: '))
b = eval(input('Number B: '))
c = eval(input('Number C: '))

if a == b and b == c:
    finres = 3 * (a + b + c)
else:
    finres = a + b + c

print(finres)

#8.
print('8.')
n = int(input('Enter Integer: '))
if n % 2 == 0:
    print('Number is Even')
else:
    print('Number is Odd')

#9.
print('9.')
l = input('Enter Letter: ')
vowel = ['A', 'E', 'I', 'O', 'U', 'a', 'e', 'i', 'o', 'u']
if l in vowel:
    print('Letter is Vowel')
else:
    print('Letter is Consonant')
    
#10.
print('10.')
val = [3,2,6,5,9]

n1 = 5
if n1 in val:
    print('Value in Group')
else:
    print('Value not in Group')

n2 = 8
if n2 in val:
    print('Value in Group')
else:
    print('Value not in Group')


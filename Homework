#17.
print('17.')
amt = 10000
perc = 3.5
years = 7

ci = amt * ((1 + (perc / 100)) ** years) - amt
fin = ci + amt


print('{:.2f}'.format(fin))

#18.
import math
print('18.')
x1, y1 = 5, 7
x2, y2 = 8, 3

dist = math.sqrt(  ((y2 - y1) ** 2)  +  ((x2 - x1) ** 2)  )
print(dist)


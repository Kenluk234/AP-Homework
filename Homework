#1.
import math
print('1.')
r = eval(input('Enter Radius: '))
area = math.pi * (r**2)
print(area)

#2.
print('2.')
first = input('Your First Name: ')
last = input('Your Last Name: ')

name = first + ' ' + last
rev_name = name[::-1]
print(rev_name)


#23.
print('23.')
x1, y1 = 3, 6
x2, y2 = 7, 2

xmid = (x1 + x2) / 2
ymid = (y1 + y2) / 2

print('Midpoint: '+'('+str(xmid)+','+str(ymid)+')')

#24.
print('24.')
for i in range (2001, 3200):
